<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

	<camelContext xmlns="http://camel.apache.org/schema/blueprint">
		<route>
			<from uri="timer:simpleTimer?fixedRate=true&amp;period=10000"/>
			<setHeader headerName="randomNumber">
				<simple>RandomNumber: ${random(1,1000)}</simple>
			</setHeader>
			<to uri="log:simpleLog?level=INFO&amp;showHeaders=true&amp;showProperties=true" />
		</route>
	
	</camelContext>
</blueprint>